{
  "name": "smilo-wallet",
  "version": "0.2.4",
  "author": "Smilo Platform",
  "homepage": "http://smilo.io/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "test": "karma start ./test-config/karma.conf.js",
    "test-ci": "karma start ./test-config/karma.conf.js --coverage --single-run",
    "e2e": "cypress open",
    "e2e-ci": "nohup ionic serve --no-open --no-livereload --no-interactive &> /dev/null & (wait-on http://localhost:8100 && cypress run && pkill -f ionic)",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "release:android": "./scripts/release-android.sh",
    "release:ios": "./scripts/release-ios.sh",
    "upload-hockeyapp:android": "./scripts/upload-to-hockey-app-android.sh",
    "upload-hockeyapp:ios": "./scripts/upload-to-hockey-app-ios.sh",
    "release-upload-hockey-app:android": "./scripts/release-android.sh && ./scripts/upload-to-hockey-app-android.sh",
    "release-upload-hockey-app:ios": "./scripts/release-ios.sh && ./scripts/upload-to-hockey-app-ios.sh",
    "postinstall": "npm run compile-bitcoin-js-wrapper && npm run compile-web-worker-lib",
    "validate-translations": "node ./scripts/translation_checker.js",
    "validate-translations:ci": "node ./scripts/translation_checker.js --ci",
    "generate-sha256-hash": "node ./scripts/generate-inline-script-hash.js",
    "compile-web-worker-lib": "tsc --p tsconfig.webworker.json && ./scripts/prepare-webworker-lib.sh",
    "compile-web-plugin-lib": "tsc --p tsconfig.webplugin.json",
    "compile-web-plugin-lib-watch": "tsc --p tsconfig.webplugin.json -w",
    "compile-bitcoin-js-wrapper": "browserify ./bitcoin-js-wrapper/wrapper.js --standalone bitcoinjs > ./src/assets/scripts/bitcoinjs-lib.js",
    "build-webplugin": "npm run generate-sha256-hash && npm run compile-web-plugin-lib && node-sass --include-path scss src/webplugin/popup.scss src/webplugin/popup.css",
    "dev-webplugin-ts": "npm run generate-sha256-hash && npm run compile-web-plugin-lib-watch",
    "dev-webplugin-scss": "node-sass -w --include-path scss src/webplugin/popup.scss src/webplugin/popup.css"
  },
